{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "build:analyze": "ANALYZE=true nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lighthouse": "lighthouse https://tecso.team --output html --output-path ./lighthouse-report.html",
    "lighthouse:mobile": "lighthouse https://tecso.team --output html --output-path ./lighthouse-mobile-report.html --preset=mobile",
    "bundle-analyzer": "npx webpack-bundle-analyzer .nuxt/dist/client --port 8888",
    "performance:audit": "npm run lighthouse && npm run bundle-analyzer",
    "docker:build": "docker build -t tecso-team .",
    "docker:run": "docker run -p 80:80 tecso-team",
    "docker:compose": "docker-compose up -d"
  },
  "dependencies": {
    "@fancyapps/ui": "^5.0.33",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/vue-fontawesome": "^3.0.6",
    "@googlemaps/js-api-loader": "^1.16.6",
    "@nuxt/image": "^1.7.0",
    "@nuxtjs/device": "^3.1.1",
    "core-js": "^3.36.0",
    "flickr-justified-gallery": "^2.2.0",
    "fullpage.js": "^4.0.20",
    "gsap": "^3.12.5",
    "isotope-layout": "^3.0.6",
    "nuxt": "^3.10.3",
    "nuxt-swiper": "^1.2.2",
    "sass": "^1.71.1",
    "sass-loader": "^14.1.1",
    "smooth-scrollbar": "^8.8.4",
    "splitting": "^1.0.6",
    "swiper": "^11.0.7",
    "update": "^0.7.4",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@vue/compiler-sfc": "^3.4.21",
    "webpack-bundle-analyzer": "^4.9.0",
    "lighthouse": "^10.4.0"
  }
}
